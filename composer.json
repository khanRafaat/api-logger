{
    "name": "rafaatabtahe/api-logger",
    "description": "A Laravel package for API response time logging and monitoring dashboard",
    "keywords": [
        "laravel",
        "api",
        "response",
        "logger",
        "monitoring",
        "dashboard"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rafaat Abtahe",
            "email": "rafaatabtahe@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "RafaatAbtahe\\ApiResponseLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RafaatAbtahe\\ApiResponseLogger\\ApiResponseLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
